{
  "name": "app",
  "version": "0.1.0",
  "scripts": {
    "build": "rescript && npx esbuild ./dist/bundle.js --bundle",
    "clean": "rescript clean -with-deps",
    "start": "npx concurrently \"yarn copy:html\" \"yarn watch:rescript\" \"yarn watch:esbuild\" \"yarn watch:live-server\"",
    "watch:rescript": "rescript build -w",
    "watch:esbuild": "esbuild ./src/Index.bs.js --outfile=./dist/bundle.js --bundle --watch",
    "watch:live-server": "node ./live-server.js",
    "copy:html": "cp ./public/index.html ./dist"
  },
  "keywords": [ "ReScript" ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "css-loader": "^5.2.6",
    "mini-css-extract-plugin": "^1.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@rescript/react": "^0.10.3",
    "typescript": "4.3.4"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "esbuild": "^0.12.12",
    "live-server": "^1.2.1",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "gentype": "^4.1.0",
    "html-webpack-plugin": "^5.3.2",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "precss": "^4.0.0",
    "prettier": "^2.3.2",
    "react-hot-loader": "^4.13.0",
    "rescript": "^9.1.4",
    "ts-loader": "^9.2.3",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
